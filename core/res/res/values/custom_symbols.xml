<resources>

    <!-- System clock rotation sensor -->
    <java-symbol type="bool" name="config_useSystemClockforRotationSensor" />

    <!-- Disabled components -->
    <java-symbol type="array" name="config_disabledComponents" />

    <!-- Force enabled components -->
    <java-symbol type="array" name="config_forceEnabledComponents" />

    <!-- Usb drive persistent notification -->
    <java-symbol type="bool" name="config_persistUsbDriveNotification" />

    <!-- Button brightness -->
    <java-symbol type="integer" name="config_buttonBrightnessSettingDefault" />
    <java-symbol type="bool" name="config_deviceHasVariableButtonBrightness" />

    <!-- Advanced reboot -->
    <java-symbol type="string" name="reboot_to_recovery_title" />
    <java-symbol type="string" name="reboot_to_recovery_message" />
    <java-symbol type="string" name="reboot_to_bootloader_title" />
    <java-symbol type="string" name="reboot_to_bootloader_message" />
    <java-symbol type="string" name="reboot_title" />
    <java-symbol type="string" name="reboot_message" />

    <!-- Vendor warning -->
    <java-symbol type="string" name="system_error_vendorprint" />

    <!-- IME selector notification -->
    <java-symbol type="bool" name="config_show_cmIMESwitcher"/>

    <!-- ADB notifications -->
    <java-symbol type="string" name="adb_net_active_notification_title" />
    <java-symbol type="string" name="adb_both_active_notification_title" />
    <java-symbol type="string" name="adb_active_generic_notification_message" />

    <!-- Link volume notification -->
    <java-symbol type="drawable" name="ic_audio_notification_new" />
    <java-symbol type="drawable" name="ic_audio_notification_mute_new" />

    <!-- Display for radio tech -->
    <java-symbol type="bool" name="config_display_rat" />

    <!-- 2G/3G/4G RAT strings for carriers -->
    <java-symbol type="string" name="config_rat_unknown" />
    <java-symbol type="string" name="config_rat_2g" />
    <java-symbol type="string" name="config_rat_3g" />
    <java-symbol type="string" name="config_rat_4g" />

    <!-- Notify fingerprint client of successful cancelled authentication -->
    <java-symbol type="bool" name="config_notifyClientOnFingerprintCancelSuccess" />

    <!-- Device key handler -->
    <java-symbol type="string" name="config_deviceKeyHandlerLib" />
    <java-symbol type="string" name="config_deviceKeyHandlerClass" />

    <!-- Dash charging support -->
    <java-symbol type="bool" name="config_hasDashCharger" />

    <!-- Whether device has turbo power charging support -->
    <java-symbol type="bool" name="config_hasTurboPowerCharger" />

    <!-- DUI -->
    <java-symbol type="anim" name="du_app_in" />
    <java-symbol type="anim" name="du_app_out" />

    <!-- Hardware key binding -->
    <java-symbol type="integer" name="config_deviceHardwareKeys" />

    <!-- Screenrecord -->
    <java-symbol type="bool" name="config_enableScreenrecordChord" />

    <!-- Emergency button in power menu -->
    <java-symbol type="bool" name="config_showEmergencyButtonInPowerMenu" />

    <!-- Define optional package verifier name -->
    <java-symbol type="string" name="config_optionalPackageVerifierName" />

    <!-- Sms authorization request to timeout -->
    <java-symbol type="integer" name="config_sms_authorization_timeout_ms" />

    <!-- Enable sms authorization framework -->
    <java-symbol type="bool" name="config_sms_authorization_enabled" />

	<!-- Pocket Lock -->
    <java-symbol type="layout" name="pocket_lock_view_layout" />
    <java-symbol type="id" name="pocket_hint_container" />
    <java-symbol type="id" name="pocket_hint_image" />
    <java-symbol type="id" name="pocket_hint_message" />

    <!-- Force doze notifications -->
    <java-symbol type="bool" name="config_canForceDozeNotifications" />

    <!-- Doze always on default value -->
    <java-symbol type="bool" name="config_dozeAlwaysOnDisplayEnabledDefault" />

    <!-- Notification icon/text dynamic tint -->
    <java-symbol type="bool" name="config_allowNotificationIconTextTinting" />
    <java-symbol type="color" name="notification_text_default_color" />

    <!-- Notification sender text color -->
    <java-symbol type="color" name="sender_text_color" />

    <!-- Vendor platform signature -->
    <java-symbol type="array" name="config_vendorPlatformSignatures" />

    <!-- On-The-Go -->
    <java-symbol type="drawable" name="ic_lock_onthego" />

    <!-- Toast text and bg colors -->
    <java-symbol type="color" name="toast_bg_color" />
    <java-symbol type="color" name="toast_text_color" />

    <!-- Forcefully tint notification icon text -->
    <java-symbol type="bool" name="config_useDarkBgNotificationIconTextTinting" />

    <!-- for notification overlays to force what they need -->
    <java-symbol type="bool" name="config_useDarkBgNotificationTinting_override" />
    <java-symbol type="bool" name="config_notificationTinting_override" />

    <!-- Updater snackbar text color -->
    <java-symbol type="color" name="updater_snackbar_text_color" />

    <!-- Whether the device supports Smart Pixels -->
    <java-symbol type="bool" name="config_enableSmartPixels" />

    <!-- Settings icon tint color -->
    <java-symbol type="color" name="settings_icon_tint_color" />

    <!-- Full screen aspect ratio -->
    <java-symbol type="bool" name="config_haveHigherAspectRatioScreen" />
    <java-symbol type="dimen" name="config_screenAspectRatio" />

    <!-- Sensor block per-package -->
    <java-symbol type="array" name="config_blockPackagesSensorDrain" />

</resources>
